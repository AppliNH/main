                                "type": "string"
                            },
                            "contact": {
                                "type": "string"
                            },
                            "hist_precedents": {
                                "type": "string"
                            },
                            "strategy": {
                                "type": "string"
                            },
                            "video_url": {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            },
                            "unit_of_measures": {
                                "type": "string"
                            }
                        },
                        "additionalProperties": true,
                        "type": "object"
                    },
                    "survey_schema": {
                        "properties": {
                            "sys_account_project_ref_id": {
                                "type": "string"
                            },
                            "support_role_types": {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            },
                            "user_need_types": {
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            }
                        },
                        "additionalProperties": true,
