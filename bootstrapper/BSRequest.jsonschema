                            },
                            "logo_upload_bytes": {
                                "type": "string"
                            }
                        },
                        "additionalProperties": true,
                        "type": "object"
                    },
                    "project_details": {
                        "properties": {
                            "sys_account_project_ref_id": {
                                "type": "string",
                                "description": "account project id"
                            },
                            "sys_account_org_ref_id": {
                                "type": "string",
                                "description": "account org id"
                            },
                            "goal": {
                                "type": "string"
                            },
                            "already_pledged": {
                                "oneOf": [
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "type": "string"
                                    }
                                ]
                            },
                            "action_time": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "action_location": {
                                "type": "string"
                            },
                            "min_pioneers": {
                                "oneOf": [
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "type": "string"
                                    }
                                ]
                            },
                            "min_rebels_media": {
                                "oneOf": [
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "type": "string"
                                    }
                                ]
                            },
                            "min_rebels_to_win": {
                                "oneOf": [
                                    {
                                        "type": "integer"
                                    },
                                    {
                                        "type": "string"
                                    }
                                ]
                            },
                            "action_length": {
                                "type": "string"
                            },
                            "action_type": {
                                "type": "string"
                            },
