{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "properties": {
        "orgs": {
            "items": {
                "$schema": "http://json-schema.org/draft-04/schema#",
                "properties": {
                    "org": {
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "logo_filepath": {
                                "type": "string"
                            },
                            "contact": {
                                "type": "string"
                            },
                            "creator_id": {
                                "type": "string"
                            },
                            "logo_upload_bytes": {
                                "type": "string"
                            }
                        },
                        "additionalProperties": true,
                        "type": "object"
                    }
                },
                "additionalProperties": true,
                "type": "object"
            },
            "type": "array"
        },
        "projects": {
            "items": {
                "$schema": "http://json-schema.org/draft-04/schema#",
                "properties": {
                    "project": {
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "logo_filepath": {
                                "type": "string"
